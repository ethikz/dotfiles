# Basic Bins (including Homebrew)
export PATH=$PATH:/bin:/usr/bin:/sbin:/usr/sbin:/usr/local/bin:/usr/local/sbin

# Git
export PATH=$PATH:/usr/local/git/bin

# NodeJS
export PATH=$PATH:/usr/local/share/npm/bin:/usr/local/lib/node_modules

# RVM
export PATH=$PATH:$HOME/.rvm/bin

# SDKMan
export SDKMAN_DIR=$HOME/.sdkman

# Home .bin Folder
export PATH=$PATH:$HOME/.bin

# Java
export PATH=$GRADLE_HOME/bin:$PATH
export PATH=$JAVA_HOME/bin:$PATH

# ICU4C
export PATH=$PATH:/usr/local/opt/icu4c/bin:/usr/local/opt/icu4c/sbin

# Python
export PATH=$PATH:/usr/local/opt/python/libexec/bin
